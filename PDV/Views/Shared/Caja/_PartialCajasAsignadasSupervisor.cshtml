@{
    string fechaInicio = DateTime.Now.AddMonths(-1).ToString("dd/MM/yyyy");
    string fechaFin = DateTime.Now.ToString("dd/MM/yyyy");
}
<div class="panel-group">
    <div class="panel panel-default">
        <div class="panel-heading" style="font-weight:bold">Filtro búsqueda:</div>
        <div class="panel-body form-inline">
            <div class="form-group" style="padding-right: 10px">
                <label for="txtFechaInicio" style="width: 120px">Fecha inicio:</label>
                <div class="input-group">
                    <input type="text" class="form-control datetime" style="max-width:none" id="txtFechaInicio" placeholder="Fecha inicio búsqueda" value="@fechaInicio" readonly />
                    <button class="input-group-addon" id="btnFechaInicio"><i class="glyphicon glyphicon-calendar"></i></button>
                </div>
            </div>
            <div class="form-group" style="padding-right: 10px">
                <label for="txtFechaFinal" style="width: 120px">Fecha término:</label>
                <div class="input-group">
                    <input type="text" class="form-control datetime" style="max-width:none" id="txtFechaFinal" placeholder="Fecha término búsqueda" value="@fechaFin" readonly />
                    <button class="input-group-addon" id="btnFechaFinal"><i class="glyphicon glyphicon-calendar"></i></button>
                </div>
            </div>
            <button class="btn btn-primary" id="btnBuscar" title="Buscar" onclick="AdministraCajaSupervisor.BuscarCajasAsignadasHistorico()"><i class="glyphicon glyphicon-search"></i></button>
        </div>
    </div>
    <div class="panel panel-default">
        <div class="panel-heading" style="font-weight:bold">Resultados:</div>
        <div class="panel-body">
            <table id="listaCajasAsignadasHistorico" class="table table-striped table-hover table-condensed compact" style="width: 100%">
                <thead>
                    <tr class="bg-info">
                        <th style="width: 20%">Caja</th>
                        <th>Función</th>
                        <th>Sección</th>
                        <th>Piso</th>
                        <th>Usuario</th>
                        <th>Fecha Asignación</th>
                        @*<th>Fecha Inicio</th>*@
                        <th>Fecha Estado</th>
                        <th>Estado</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>


<script type="text/javascript">
    $(document).ready(function () {
        AdministraCajaSupervisor.InitCajasHistorico();
    });
</script>