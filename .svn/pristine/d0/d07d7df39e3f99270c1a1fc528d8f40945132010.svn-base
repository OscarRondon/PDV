<div class="panel panel-default">
    <div class="panel-heading">
        <table>
            <tr>
                <td style="font-weight: bold; width: 10%">Nómina N°:</td>
                <td style="width: 10%"><span id="spnNumeroNomina"></span></td>
                <td style="font-weight: bold; width: 10%">Tipo Nómina:</td>
                <td><span id="spnTipoNomina"></span></td>
            </tr>
        </table>
    </div>
    <div class="panel-body">
        <table id="listaDetalleNomina" class="table table-striped table-hover table-condensed compact nowrap" style="width: 100%">
            <thead>
                <tr class="bg-info">
                    <th>N° Orden Atención</th>
                    <th>Fecha</th>
                    <th>Tipo Documento</th>
                    <th>N° Documento</th>
                    <th>Rut Paciente</th>
                    <th>Nombre Paciente</th>
                    <th>Rut Titular</th>
                    <th>Nombre Titular</th>
                    <th>Ficha Clínica</th>
                    <th>Episodio</th>
                    <th>Valor</th>
                    <th>Estado</th>
                    <th>Comentario</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div class="panel-footer text-right" style="display: none" id="pnlBotones">
        <button type="button" class="btn btn-success btn-sm" id="btnReenviarTodos" onclick="NominaDetalleSupervisor.ConfirmarReenviarTodas()"><span class="glyphicon glyphicon-share-alt"></span>&nbsp;Reenviar todos</button>
        <button type="button" class="btn btn-danger btn-sm" id="btnRechazarTodos" onclick="NominaDetalleSupervisor.ConfirmarRechazarTodas()"><span class="glyphicon glyphicon-remove"></span>&nbsp;Rechazar todos</button>

        <button type="button" class="btn btn-primary btn-sm" id="btnGuardarAprobaciones" onclick="NominaDetalleSupervisor.ValidarDocumentos()">Guardar documentos</button>
    </div>
</div>

<script type="text/javascript" src="~/Scripts/HMS/Nomina/NominaSupervisorDetalle.js"></script>
